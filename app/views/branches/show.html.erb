<div class="row-fluid">
	<div class="span9">
		<div class="row-fluid">
			<div class="well">
				<p class="lead text-center"><legend><strong>分支机构</strong></legend></p>
				<%= simple_form_for @branch, :html => { :class => 'form-horizontal' } do |f| %>
					<%= render 'fields', :f => f %>
				<% end %>
			</div>
		</div>
		<div class="row-fluid">
			<p>
				<div class="alert alert-success">
					所辖居间人
				</div>
				<%= render :partial => 'branches/branch_brokers' %>
			</p>
		</div>
	</div>
	<div class="span3">
		<div class="alert alert-block">
		  <button type="button" class="close" data-dismiss="alert">&times;</button>
		  <h4>帮助</h4>
		  <p>查看分支机构情况及下属居间人</p>
		</div>
	</div>
</div>
